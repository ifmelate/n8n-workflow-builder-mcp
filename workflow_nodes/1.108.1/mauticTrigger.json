    {
      "name": "authentication",
      "displayName": "Authentication",
      "type": "options",
      "default": "credentials",
      "options": [
        {
          "name": "Credentials",
          "value": "credentials"
        },
        {
          "name": "OAuth2",
          "value": "oAuth2"
        }
      ]
    },
    {
      "name": "events",
      "displayName": "Event Names or IDs",
      "type": "multiOptions",
      "default": [],
