        {
          "effect": "show",
          "key": "include",
          "values": [
            "allFieldsExcept"
          ]
        }
      ]
    },
    {
      "name": "fieldsToInclude",
      "displayName": "Fields To Include",
      "type": "string",
      "default": "",
      "placeholder": "e.g. email, name",
      "displayOptions": {
        "show": {
          "aggregate": [
            "aggregateAllItemData"
