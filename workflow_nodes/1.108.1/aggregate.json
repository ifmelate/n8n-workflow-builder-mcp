        "show": {
          "aggregate": [
            "aggregateAllItemData"
          ],
          "include": [
            "allFieldsExcept"
          ]
        }
      },
      "visibilityRulesNormalized": [
        {
          "effect": "show",
          "key": "aggregate",
          "values": [
            "aggregateAllItemData"
          ]
