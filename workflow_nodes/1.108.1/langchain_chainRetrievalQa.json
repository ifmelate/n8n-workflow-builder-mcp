{
  "nodeType": "@n8n/n8n-nodes-langchain.chainRetrievalQa",
  "displayName": "Question and Answer Chain",
  "description": "Answer questions about retrieved documents",
  "version": [
    1,
    1.1,
    1.2,
    1.3,
    1.4,
    1.5,
    1.6
  ],
  "properties": [
    {
      "name": "query",
      "displayName": "Query",
      "type": "string",
      "default": "={{ $json.input }}",
      "required": true,
      "displayOptions": {
        "show": {
          "@version": [
            1
          ]
        }
      },
      "visibilityRulesNormalized": [
        {
          "effect": "show",
          "key": "@version",
          "values": [
            1
          ]
        }
      ]
    },
    {
      "name": "text",
      "displayName": "Prompt (User Message)",
      "type": "string",
      "default": "",
      "placeholder": "e.g. Hello, how can you help me?",
      "required": true,
      "typeOptions": {
        "rows": 2
      },
      "displayOptions": {
        "show": {
          "promptType": [
            "define"
          ]
        }
      },
      "visibilityRulesNormalized": [
        {
          "effect": "show",
          "key": "promptType",
          "values": [
            "define"
          ]
        }
      ]
    },
    {
      "name": "options",
      "displayName": "Options",
      "type": "collection",
      "default": {},
      "placeholder": "Add Option",
      "displayOptions": "{\n\t\t\t\t\t\t\tshow: {\n\t\t\t\t\t\t\t\t'@version': [{ _cnd: { lt: 1.5 }"
    }
  ],
  "credentialsConfig": [
    {
      "name": "query",
      "required": true
    },
    {
      "name": "text",
      "required": true
    }
  ],
  "io": {
    "inputs": [
      "Main",
      "AiLanguageModel",
      "AiRetriever"
    ],
    "outputs": [
      "Main"
    ],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [
      "Main",
      "AiLanguageModel",
      "AiRetriever"
    ],
    "produces": [
      "Main"
    ]
  },
  "inputHints": "Accepts inputs: Main, AiLanguageModel, AiRetriever",
  "outputHints": "Emits outputs: Main"
}